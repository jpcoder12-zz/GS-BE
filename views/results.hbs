<div id="header">
      <img id="logo" src="img/google.png" />
      <form id="search">
        <input type="text" name="results-search" />
        <button name="input-search">Search</button>
      </form>
    </div>

    <div id="search-results">
      <div>
        <span id="result-count">{{data.length}} Results</span>
      </div>
      <!-- Result -->
      {{#each data as |element|}}
      <section>
        <small>{{element.url}}</small>
        <a href={{element.url}} target="_blank"
          ><h2>{{element.title}}</h2></a
        >
        <p>
          {{element.description}}
        </p>
        <ul>
          {{#each links as |link|}}
          <li>
            <a href={{link.url}} target="_blank"
              >{{link.title}}</a
            >
          </li>
          {{/each}}
        </ul>
      </section>
      {{/each}}
    </div>