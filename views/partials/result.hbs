{{#each data as |element|}}
      <article>
        <small>{{element.url}}</small>
        <a href={{element.url}} target="_blank"
          ><h2>{{element.title}}</h2></a
        >
        <p>
          {{element.description}}
        </p>
        <ul>
          {{#each links as |link|}}
          <li>
            <a href={{link.url}} target="_blank"
              >{{link.title}}</a
            >
          </li>
          {{/each}}
        </ul>
      </article>
      {{/each}}